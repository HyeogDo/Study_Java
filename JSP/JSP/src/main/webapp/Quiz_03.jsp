<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>
<!DOCTYPE html>
<html>
	<head>
		<meta charset="UTF-8">
		<title>Insert title here</title>
	</head>
	<body>
		<%
		int i = 1;
		int sum = i;
		while(i < 100) {
			i++;
			sum += i;
		}
		%>
		1부터 100까지의 합 = <% out.print(sum); %><br>
		<%
		while(i < 200) {
			i++;
			sum += i;
		}
		%>
		1부터 200까지의 합 = <% out.print(sum); %><br>
	</body>
</html>