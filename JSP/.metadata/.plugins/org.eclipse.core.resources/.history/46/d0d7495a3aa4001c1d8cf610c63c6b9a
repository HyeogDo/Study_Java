		<%!
			public void power(int res) {
				res*=2;
			}
		%>
		
		<%
			int res = 1;
			for (int i = 1; i <= 5; i++ ) {
				power(res);
				out.print(2 + " ^ " + i + " = " + res + "<br>");
			}
		%>
		

