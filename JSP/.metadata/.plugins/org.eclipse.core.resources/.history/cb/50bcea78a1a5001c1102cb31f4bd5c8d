<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>
<!DOCTYPE html>
<html>
	<head>
		<meta charset="UTF-8">
		<title>move</title>
	</head>
	<body>
		<%
			String aaa = request.getParameter("siteName");
		
			switch(aaa) {
				case "naver":
					response.sendRedirect("https://www.naver.com");
					break;
				case "daum":
					response.sendRedirect("https://www.daum.com");
					break;
					
				default:
					response.sendRedirect("https://www.yahoo.com");
					break;
				
			}
		%>	
	</body>
</html>