package com.javalec.base;

/*
 * date : 2022/02/24
 * name : HyeogDo
 * purpose : study while
 */

public class While {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int i = 1;
		int tot = 0;
		
		
		// 1부터 100까지의 합을 구하기
		while ( i <= 100) {
			tot += i;
			i++;
		}
		System.out.println(tot);
	}

}
