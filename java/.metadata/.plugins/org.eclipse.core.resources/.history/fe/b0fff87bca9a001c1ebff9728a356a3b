public class StoreShop01 extends StoreHQ{
	
	// Field
	
	// Constructor
	public StoreShop01() {
		// TODO Auto-generated constructor stub
	}
	
	// Method
	
	// 김치찌개가 4500원
	@Override
	public void orderKim() {
		// TODO Auto-generated method stub
		super.orderKim();
		System.out.println("김치찌개(shop1) : 4500원");
	}
	
	// 부대찌개 5000원
	@Override
	public void orderBu() {
		// TODO Auto-generated method stub
		System.out.println("부대찌개(shop1) : 6000원");
	}
	
	
	// 순대국 : 판매하지 않습니다.
	@Override
	public void orderSoon() {
		// TODO Auto-generated method stub
		System.out.println("순대국은 판매하지 않습니다.");
	}
}
