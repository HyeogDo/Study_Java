		// 1부터 10까지의 수중 홀수만 출력하기
		for ( int i = 1; i <= 10; i++ ) {
			if (i % 2 == 1) {
				System.out.println(i);
			}
		}
		
		for (int i = 1; i <= 10; i += 2) {
			System.out.println(i);
		}
		
		
		
	}

