* 최종 수정일 : 2022/03/02
* 목적 : 객체지향 프로그래밍 연습
*/

public class Calc {
	
	// Field
	int num1;
	int num2;
	
	// Constructor
	public Calc() {
		// TODO Auto-generated constructor stub
	}
	
	// Method
	
	public void addNum() {
		System.out.println(num1 + " + " + num2 + " = " + (num1 + num2) );
	}
	public void minusNum() {
		System.out.println(num1 + " - " + num2 + " = " + (num1 - num2) );
	}
	public void crossNum() {
		System.out.println(num1 + " * " + num2 + " = " + (num1 * num2) );
	}
	public void devideNum() {
		System.out.println(num1 + " / " + num2 + " = " + (num1 / num2) );
	}
	

}
