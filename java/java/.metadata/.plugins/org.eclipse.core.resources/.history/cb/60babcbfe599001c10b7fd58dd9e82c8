public class Addition {
	
	// Field
	
	
	// Constructor
	public Addition() {
		// TODO Auto-generated constructor stub
	}
	
	// Method
	public int sumNum(int num1, int num2) {
		int sum = num1 + num2;
		
		return sum;
	}
	
	
}
